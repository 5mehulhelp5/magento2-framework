{
    "name": "magedin/magento2-framework",
    "description": "A set of utilities to help in Magento 2 development.",
    "type": "magento2-library",
    "license": "MIT",
    "authors": [
        {
            "name": "MagedIn Technology",
            "email": "support@magedin.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4||^8.0",
        "magento/framework": "*"
    },
    "autoload": {
        "psr-4": {
            "MagedIn\\Framework\\Magento2\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    },
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true
        }
    }
}
